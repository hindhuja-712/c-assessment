#include<stdio.h>
int main()
{
    int num=0,flag=0;
    for(int i=2;i<10;i++)
    {
        flag=0;
        for(int j=2;j<i;j++)
        {
            if(i%j==0)
            {
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            num++;
        }
    }
    printf("%d",num);
    
}