#include<stdio.h>
int main()
{
    int a,flag=0,sum=0,copy;
    scanf("%d",&a);
    copy=a;
    for(;copy>0;copy/=10)
    {
        sum=sum+copy%10;
    }
    for(int i=2;i<a;i++)
    {
        flag=0;
        if(a%i==0)
        {
            flag=1;
            break;
        }
    }
    if(flag==0&&sum==14)
    {
        printf("Prime&sum of digit is 14");
    }
    else if(flag==0&&sum!=14)
    {
        printf("prime &sum of digit is not 14");
    }
    else if(flag==1&&sum==14)
    {
        printf("Not prime & Sum of the digit is 14");
    }
    else if(flag==1&&sum!=14)
    {
        printf("Not prime but sum of the digit is not 14");
    }
}