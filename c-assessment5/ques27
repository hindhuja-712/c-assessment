#include<stdio.h>
int main()
{
    int sum=0,copy,num=0;
    for(int i=1;i<100000;i++)
    {
        sum=0;copy=i;
      for(;copy>0;copy/=10)
      {
          sum=sum+copy%10;
      }
          if(sum==14)
          {
              num++;
          }
      
    }
    printf("%d",num);
}