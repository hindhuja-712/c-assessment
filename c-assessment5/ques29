#include<stdio.h>
int main()
{
    int a,b,c,max=0,lcm=0;
    scanf("%d %d %d",&a,&b,&c);
    if(a>b&&a>c)max=a;
    else if(b>a&&b>c) max=b;
    else max=c;
    for(int i=max;i<=a*b*c;i++)
    {
        if(i%a==0&&i%b==0&&i%c==0)
        {
            lcm=i;
            break;
        }
    }
    printf("%d",lcm);
}