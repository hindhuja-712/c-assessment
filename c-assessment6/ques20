#include<stdio.h>
int main()
{
    int i=2,count=0,flag=0,j=2;
    while(i<10)
    {
        flag=0;
        j=2;
        while(j<i)
        {
            if(i%j==0)
            {
                flag=1;
                break;
            }
           j++;
        }
        if(flag==0)
        {
            count++;
        }
       i++; 
    }
    printf("%d",count);
}
