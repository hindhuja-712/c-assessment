#include<stdio.h>
int main()
{
    int a,rev=0,fir=0,las=0,copy,temp=1,mid=0;
    scanf("%d",&a);
    copy=a;
    las=copy%10;
    copy=a;
    while(copy>0)
    {
        temp=temp*10;
        copy/=10;
    }copy=a;
    fir=copy/(temp/10);
    mid=copy%(temp/10)/10;
    if(fir%2==0)
    {
        printf("%d",a);
    }
    else
    {
        fir=fir-1;
        rev=fir*(temp/10)+mid*10+las;
        printf("%d",rev);
    }
}