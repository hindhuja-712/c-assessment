#include<stdio.h>
int lcm=0,max=0;
void f(int a ,int b,int c)
{
    if(a>b&&a>c)max=a;
    else if(b>a&&b>c)max=b;
    else max=c;
    for(int i=max;i<=a*b*c;i++)
    {
     if(i%a==0&&i%b==0&&i%c==0)
     {
         lcm=i;
         break;
     }
    }
    printf("%d",lcm);
}
int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    f(a,b,c);
    return 0;
}
