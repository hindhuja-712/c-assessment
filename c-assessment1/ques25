#include<stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    int flag;
    flag=a;
    int las=0;
    las=flag%10;
    int fir=0;
    fir=flag/100;
    int sec=0;
    flag=a;
    sec=(flag%100)/10;
    int sum=0;
    sum=fir+sec+las;
    int temp=0;
    temp=sum/10+sum%10;
    temp=temp/10+temp%10;
    printf("%d",temp);
}