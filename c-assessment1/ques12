#include<stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    int flag;
    flag=a;
    int b=0;
    b=(flag%100)/10;
    int sum=0;
    sum=b;
    int c=0;
    flag=a;
    c=flag/100;
    sum=sum+c;
    int d=0;
    flag=a;
    d=flag%10;
    sum=sum+d;
    printf("%d",sum);
}