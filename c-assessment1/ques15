#include<stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    int rev=0;
    int flag;
    flag=a;
    int b=0;
    b=a/100;
    int copy;
    copy=b;
    int fir=0;
    fir=copy%10;
    int sec=0;
    copy=b;
    sec=copy/10;
    int c=0;
    flag=a;
    c=flag%100;
    rev=fir*1000+sec*100+c;
    printf("%d",rev);
}