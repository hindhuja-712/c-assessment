#include<stdio.h>
int main()
{
    int a;
    scanf("%d",&a);
    int flag;
    flag=a;
    int b=0;
    b=flag%100;
    int rev=0;
    int d=0;
    int copy;
    copy=b;
    d=copy%10;
    copy=b;
    int e=0;
    e=copy/10;
    rev=d*10+e;
    flag=a;
    int c=0;
    c=flag/100;
    int sum=0;
    sum=c*100+rev;
    printf("%d",sum);
}
